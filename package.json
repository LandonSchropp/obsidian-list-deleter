{
  "name": "obsidian-list-cycler",
  "version": "1.1.0",
  "description": "Cycles between different types of lists and checkboxes in Obisidan",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
