    "preinstall": "npx only-allow pnpm",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "obsidian": "latest",
    "prettier": "^3.3.3",
    "tsc-files": "^1.1.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
